{
  "name": "@ayco/personal-website",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint . --config eslint.config.mjs --cache",
    "format": "prettier . --write",
    "check": "npm run format && npm run lint",
    "astro": "astro",
    "build:preview": "astro build && astro preview",
    "deploy": "astro build && scp -r dist ayo@ayco.io:~/ayco.io-flask/",
    "copy:dist": "astro build && cp -R dist ../ayco.io-flask/",
    "prepare": "husky && husky install"
  },
  "devDependencies": {
    "@astro-reactive/form": "^0.10.0",
    "@astro-reactive/validator": "^0.5.0",
    "@astrojs/sitemap": "^3.2.1",
    "@ayco/astro-sw": "^0.8.12",
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@typescript-eslint/parser": "^8.18.2",
    "astro": "^5.0.5",
    "astro-eslint-parser": "^1.1.0",
    "astro-github-stats": "^0.7.1",
    "astro-iconify": "^1.2.0",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.18.2"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "lint-staged": {
    "*.{js,mjs,astro,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
